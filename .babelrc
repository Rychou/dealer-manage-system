{
  "presets": [
    "env",
    "stage-0",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    ["import", [{
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": "css"
    }, {
      "libraryName": "ant-design-pro",
      "libraryDirectory": "lib",
      "style": true,
      "camel2DashComponentName": false
    }]]
  ]
}